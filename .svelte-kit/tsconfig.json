{
	"compilerOptions": {
		"paths": {
			"$components": [
				"../src/components"
			],
			"$components/*": [
				"../src/components/*"
			],
			"$modules": [
				"../src/modules"
			],
			"$modules/*": [
				"../src/modules/*"
			],
			"$config": [
				"../src/config"
			],
			"$config/*": [
				"../src/config/*"
			],
			"$content": [
				"../src/content"
			],
			"$content/*": [
				"../src/content/*"
			],
			"$services": [
				"../src/services"
			],
			"$services/*": [
				"../src/services/*"
			],
			"$styles": [
				"../src/styles"
			],
			"$styles/*": [
				"../src/styles/*"
			],
			"$tokens": [
				"../src/styles/tokens"
			],
			"$tokens/*": [
				"../src/styles/tokens/*"
			],
			"$lib": [
				"../src/lib"
			],
			"$lib/*": [
				"../src/lib/*"
			],
			"$app/types": [
				"./types/index.d.ts"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "bundler",
		"module": "esnext",
		"noEmit": true,
		"target": "esnext"
	},
	"include": [
		"ambient.d.ts",
		"non-ambient.d.ts",
		"./types/**/$types.d.ts",
		"../vite.config.js",
		"../vite.config.ts",
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte",
		"../tests/**/*.js",
		"../tests/**/*.ts",
		"../tests/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"../src/service-worker.js",
		"../src/service-worker/**/*.js",
		"../src/service-worker.ts",
		"../src/service-worker/**/*.ts",
		"../src/service-worker.d.ts",
		"../src/service-worker/**/*.d.ts"
	]
}